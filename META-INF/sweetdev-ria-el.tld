      <name>multiField</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>boolean</type>
      <description>Set the method of HTML render of the selected items, in multiselect mode.</description>
    </attribute>
    <attribute>
      <name>paginable</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>boolean</type>
      <description>Set the paginable state of the suggest : if the values retrieved are truncated, display a pagination bar to get the next page from the server. (Default is false).</description>
    </attribute>
    <attribute>
      <name>preload</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>boolean</type>
      <description>Preload some items according to the preselectedValue attribute. (Default is false).</description>
    </attribute>
